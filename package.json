{
  "name": "vovklesia",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:github": "cross-env DEPLOY_TARGET=github tsc -b && cross-env DEPLOY_TARGET=github vite build",
    "build:vercel": "cross-env DEPLOY_TARGET=vercel tsc -b && cross-env DEPLOY_TARGET=vercel vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "npm run build:github && gh-pages -d dist --no-history",
    "deploy1": "git status -sb && git add -A && (git commit -m \"Р С•Р С—Р С‘РЎРѓ\" || echo No changes to commit) && git push origin main && npm run deploy",
    "deploy2": "git fetch origin && git status -sb && git add -A && (git commit -m \"Р С•Р С—Р С‘РЎРѓ\" || echo No changes to commit) && git push --force-with-lease origin main && npm run deploy",
    "vercel": "npm run build:vercel && vercel --prod --yes",
    "vercel:test": "npm run build:vercel && vercel --yes",
    "vercel:prod": "npm run build:vercel && vercel --prod --yes",
    "deploy-fast": "powershell -ExecutionPolicy Bypass -File fast-deploy.ps1"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.4",
    "daterangepicker": "^3.1.0",
    "jquery": "^3.7.1",
    "jspdf": "^3.0.1",
    "moment": "^2.30.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/jquery": "^3.5.32",
    "@types/jspdf": "^1.3.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "sass": "^1.89.0",
    "sass-embedded": "^1.89.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}